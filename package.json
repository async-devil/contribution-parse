{
  "name": "contribution-tree",
  "version": "1.0.0",
  "description": "Git conributions parse",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint",
    "start-code": "nodemon src/index.ts",
    "start-scss": "sass --watch src/public/scss:src/public/css",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/async-devil/contribution-tree.git"
  },
  "keywords": [
    "Git",
    "Parsing",
    "Visualization"
  ],
  "author": "async-devil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/async-devil/contribution-tree/issues"
  },
  "homepage": "https://github.com/async-devil/contribution-tree#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/source-map-support": "^0.5.3",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "onchange": "^7.1.0",
    "prettier": "2.1.2",
    "sass": "^1.29.0",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "hbs": "^4.1.1",
    "request": "^2.88.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "/node_modules",
      "*.js",
      "/.vscode",
      "/config",
      "*.json"
    ],
    "ext": "ts,hbs,scss"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"]
  }
}
